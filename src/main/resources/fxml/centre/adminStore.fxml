<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="centre.controller.AdminStoreController">
    <children>
        <VBox alignment="CENTER" prefHeight="765.0" prefWidth="1075.0" spacing="80.0">
            <children>
                <Button mnemonicParsing="false" onAction="#addStore" prefHeight="136.0" prefWidth="769.0"
                        text="Ajouter une boutique">
                    <font>
                        <Font size="53.0"/>
                    </font>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#editStore" prefHeight="136.0" prefWidth="769.0"
                        text="Modifier une boutique">
                    <font>
                        <Font size="53.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#deleteStore" prefHeight="136.0" prefWidth="769.0"
                        text="Supprimer une boutique">
                    <font>
                        <Font size="53.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
