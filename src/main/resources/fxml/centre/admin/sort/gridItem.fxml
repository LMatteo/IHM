<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="centre.admin.sort.GridItemController">
    <children>
        <ImageView fx:id="logo" fitHeight="130.0" fitWidth="130.0" onMouseClicked="#select" pickOnBounds="true"
                   preserveRatio="true"/>
        <Label fx:id="storeName" alignment="CENTER" onMouseClicked="#select" prefHeight="21.0" prefWidth="130.0">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
    </children>
</VBox>
